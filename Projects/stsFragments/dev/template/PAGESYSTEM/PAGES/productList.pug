//- productList.pug
extends ../LAYOUT/layout.pug
block title
	-var page={slug:'productList',title:'productList'};
	title productList
block page
	.container
		+LEVEL-productSection()
			ul.productList__list 
				+MODULE-productInline(productInline)
				+MODULE-productInline(productInline1)
				+MODULE-productInline(productInline2) 
				+MODULE-productInline(productInline3)
				+MODULE-productInline(productInline4)
				+MODULE-productInline(productInline5)
				+MODULE-productInline(productInline6)
		+LEVEL-productSection()
			ul.productCol__list.clearfix
				+MODULE-productCol(productInline)
				+MODULE-productCol(productInline1)
				+MODULE-productCol(productInline2) 
				+MODULE-productCol(productInline3)
				+MODULE-productCol(productInline4)
				+MODULE-productCol(productInline5)
				+MODULE-productCol(productInline6)