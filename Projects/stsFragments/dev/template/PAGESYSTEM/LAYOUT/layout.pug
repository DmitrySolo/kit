include ../INCLUDES/_includes
<!-- split header.html -->
doctype html
html(lang=''+head.lang)
	head
		include ../INCLUDES/_head
	body(class=''+bodyClass, data-qcontent="layout__header")
		.projectWrapper
			<!-- HEADER -->
			include _header

				<!-- split #{page.slug}.html -->
			<!-- MAIN -->
			block page
			<!-- FOOTER -->
			include _footer

			<!-- SCRIPTS -->
				<!-- split footer-scripts.html -->
			include ../SCRIPTS-STYLES/FOOTER/_footerScripts
			-if (OPTIONS.useSVGSprite=="true")
				<!-- split SVG SPRITE -->
				.hidden
					include ../../../../dist/source/icons/SVGSpriteIcons.svg
			-if (OPTIONS.accessibilityTesting =='true')
				script.
					axe.run(function (err, results) {
					console.log(results)

					});

	include ../INCLUDES/_debugPannel