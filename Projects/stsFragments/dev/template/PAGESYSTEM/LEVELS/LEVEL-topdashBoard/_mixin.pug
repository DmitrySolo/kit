mixin LEVEL-topdashBoard(data)
	-if (!className) className =''
	.topdashBoard__content(class = 'topdashBoard__content'+ className data-qcontent="level__topdashBoard")
		ul.topdashBoard__list.group.group--el    
			+dashBoardElement('tie','Личный Кабинет   ','dashBoardElement')
			+dashBoardElement('notification','Уведомления','dashBoardElement dashBoardElement--notice','noticer')
			+dashBoardElement('help','Помощь и настройки','dashBoardElement')
			+dashBoardElement('ptnMap','Пункты выдачи','dashBoardElement')
			// +dashBoardElement('orderlist','Заявка:','dashBoardElement','orderList')  
			+dashBoardElement('help','Режим Торгового Зала','dashBoardElement','switcher') 
	if block
		block 