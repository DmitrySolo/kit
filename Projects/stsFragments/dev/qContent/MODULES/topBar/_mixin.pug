mixin MODULE-topBar()
	<!-- split modules/topBar -->
	.topBar__content(data-qcontent='module__topBar')
		.group.group--md.group--v-md.group--va-middle    
			.topBar
				.col.col-4-tl.col-16-m
					.topBar__catalog
						+MODULE-catalog() 
				.col.col-8-tl
					.topBar__search
						+MODULE-mainSearch()   
				.col.col-4-tl.showFrom-tl
					.topBar__cart 
						+MODULE-cart() 
			 
				