mixin MODULE-topBar()
	<!-- split modules/topBar -->
	.topBar__content(data-qcontent='module__topBar')
		.group.group--md.group--v-md.group--va-middle.topBar    
				.col.col-3-tl.col-12-m
					.topBar__catalog 
						+MODULE-catalog() 
				.col.col-6-tl.showFrom-tl
					.topBar__search
						+MODULE-mainSearch('')   
				.col.col-3-tl.showFrom-tl.no-gutter-l 
					.topBar__cart
						+B-compare()
						+MODULE-cart()