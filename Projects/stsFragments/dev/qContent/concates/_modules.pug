mixin LEVEL-productInfo(data)
	.productInfo
		.productInfo__content(class = 'productInfo__content'+ className data-qcontent="level__productInfo")
			.productMain
				.group.group--md
					.col.col-5-tl
						+MODULE-productImageSlider()
					.col.col-3-tl.col-6-tp
						.productInfo__section
							.productInfo__priceStock
								span.productCol__price 14 300
									span.icon-ruble
								span.productCol__stock.icon-tick В наличии:
							+B-toOrder('В корзину')
							+B-button('Оформить без регистрации','fastBuy')
							+MODULE-productChar()
					.col.col-4-tl.col-6-tp
						.productInfo__section
							+MODULE-deliverySection()
						
			.productLogo
				img(src='https://santehsmart.ru/upload/resize_cache/uf/663/120_40_1/663573ea5278ca044fad90b0a4f2346d.png')
//
mixin LEVEL-productSection(title,className)
	.productSection__content(data-qcontent="level__productSection")
		header.productSection__header.clearfix
			.productSection__pager.f-left 
				//span.productSection__subTitle Страницы:
				+breadcrumbs()
			.productSection__filter.f-left   
				span.productSection__subTitle Cортировать по:
				a.productSection__sortBy.productSection__sortBy--selected Цене
				| |
				a.productSection__sortBy Производителю
			.productSection__layoutChoiser.f-right
				span.productSection__subTitle Вид каталога:
				a(href='/')
					+I('layout','productsLayout icon--productsLayout--selected')
				a(href='/')
					+I('layout1','productsLayout') 
		if block
			block
