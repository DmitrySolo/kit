{
	"colors": {
		"'primary'": "#3b68c9",
		"'secondary'": "#ebebeb",
		"'accent'": "#00aab5",
		"'fground'": "#ebebeb",
		"'bground'": "#ffffff",
		"'font'": "#000000",
		"'hl'": "#e32a8e\n)"
	},
	"color-step": ".15",
	"colors-map": "map-merge($colors-map,$colors-map1)",
	"colors-map1": {
		"'#{$role}'": "map-get($colors",
		"'#{$role}-l'": "lighten(map-get($colors",
		"'#{$role}-d'": "darken(map-get($colors",
		"'#{$role}-t'": "transparentize(map-get($colors"
	},
	"key": {
		"$inc": "$colors-map) {\n        @return map-get($inc"
	}
}