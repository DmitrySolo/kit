mixin catalogItemsonCatPage()
	<!-- split modules/catalogItemsonCatPage -->
		-var i = 1
		+loop(11)
			.col-6-m
				.catalogItem
					.catalogItem__imgCtn
						a(href="#")
							img.catalogItem__img(src="images/catalog/" + i + ".jpg")
					.catalogItem__title
						a(href='/#') Набор разделочных досок Joseph Joseph Index Mini 60098 графит
					.catalogItem__priceAndBuy__price
						span 30 400Р
					.catalogItem__priceAndBuy__buy
						+B-toCart()
				-i++