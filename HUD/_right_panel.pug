.debug-Pannel-Right.notEdit
    .debug-Pannel-Right__section.debugPannel__section
        .debug-Pannel-Right-content

            h6
                +I('target')
                | Classes
            div.clearfix
                .col-6-tp
                    label Block
                        input(type='text').classChoser
                    label Element
                        input(type='text').classChoser
                    label Modifier
                        input(type='text').classChoser
                    label Optional
                        input(type='text').classChoser
                .col-6-tp
                    label Vendor classes
                        select(multiple='multiple',size='5')
                            option hide
                            option show
                            option clearfix
                            option mode
                            option active
                            option platform
                            option selected
                            option additional
                            option support
                    label Project classes
                        select(multiple='multiple',size='4')
                            option hide
                            option show
                            option clearfix
                            option mode
                            option active

                hr
            h6
                +I('column4')
                | Grids
            div.clearfix
                .col-2-tp
                    +I('computer3')
                    +I('computer2')
                    +I('computer1')
                    +I('computer')
                    +I('arrow')
                .col-2-tp
                    input( type="text" value=0 name="mobileGridder")
                    input( type="text" value=0 name="tpGridder")
                    input( type="text" value=0 name="tlGridder")
                    input( type="text" value=0 name="desctopGridder")
                    input( type="text" value=0 name="bigDesctopGridder")
                .col-8-tp
                    input( type="range" min="0" max="12" step="1" list="tickmarks" onchange="gridder('mobileGridder',this.value)" value="0")
                    input( type="range" min="0" max="12" step="1" list="tickmarks" onchange="gridder('tpGridder',this.value)" value="0")
                    input( type="range" min="0" max="12" step="1" list="tickmarks" onchange="gridder('tlGridder',this.value)" value="0")
                    input( type="range" min="0" max="12" step="1" list="tickmarks" onchange="gridder('desctopGridder',this.value)" value="0")
                    input( type="range" min="0" max="12" step="1" list="tickmarks" onchange="gridder('bigDesctopGridder',this.value)" value="0")
                hr
            h6
                +I('computer3')
                +I('computer2')
                +I('computer1')
                +I('computer')
                +I('arrow')

            div.clearfix

            h6
                +I('ruler')
                | Size
            div
                button#100p 100%
                hr
            h6
                +I('text')
                | Text & Font
            div
                button#textAC center
                button#textAL left
                button#textAR right
                hr
            h6
                +I('mp')
                | Paddings & Margins
            div
                input( type="range" min=".1" max="2" step=".2" list="tickmarks" onchange="padder(this.value)")
                input( type="range" min=".1" max="2" step=".2" list="tickmarks" onchange="padderrl(this.value)")
                input( type="range" min=".1" max="2" step=".1" list="tickmarks" onchange="paddertb(this.value)")
                hr
                input( type="range" min=".1" max="2" step=".2" list="tickmarks" onchange="marginer(this.value)")
                input( type="range" min=".1" max="2" step=".2" list="tickmarks" onchange="marginerrl(this.value)")
                input( type="range" min=".1" max="2" step=".2" list="tickmarks" onchange="marginertb(this.value)")
            h6
                +I('boxborder')
                | Border & Box
            div
            h6
                +I('image')
                | Background
            div

