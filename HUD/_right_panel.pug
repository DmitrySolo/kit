.classMaker.rightPlTab
    section#context
    section#classSelector
    .combinedSelector
        section#selfProperties
        section#extendsSelectors
.showFonts.rightPlTab.hidden
    section.fontFilter
        span.filterButton.filterButton--cat handwriting
        span.filterButton.filterButton--cat Serif
        span.filterButton.filterButton--cat Sans-serif
        span.filterButton.filterButton--cat Display
        span.filterButton.filterButton--lan Latin
        span.filterButton.filterButton--lan Cyrillic
    section.fontList
        ul
section.row.hidden
    div.clearfix
        .col-6-tp
            label Block
                input(type='text' name='block').classChoser
            label Element
                input(type='text' name="element").classChoser
            label Modifier
                input(type='text' name="modifier").classChoser
            label Optional
                input(type='text' mame="optionalClass").classChoser
        .col-6-tp
            label Vendor classes
                select(multiple='multiple',size='5' name="vendorName")
                    option hide
                    option show
                    option clearfix
                    option mode
                    option active
                    option platform
                    option selected
                    option additional
                    option support
            label Project classes
                select(multiple='multiple',size='4')
                    option hide
                    option show
                    option clearfix
                    option mode
                    option active
