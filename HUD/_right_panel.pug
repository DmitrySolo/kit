div.clearfix.mediaChoiser.clearfix
            .hud-Button.f-left.on#css
                +I('css')
            .hud-Button.f-left#font
                +I('font')
            .iconMediachoiser.f-left.mediaTabletP.col-2-tp
                +I('computer2')
            .iconMediachoiser.f-left.mediaTabletL.col-2-tp
                +I('computer1')
